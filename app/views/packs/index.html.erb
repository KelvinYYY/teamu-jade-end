<p id="notice"><%= notice %></p>

<h1>Listing Packs</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>City</th>
      <th>State</th>
      <th>Population</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @packs.each do |pack| %>
      <% pack=toPack(pack) %>
      <tr>
        <td><%= pack.id %></td>
        <td><%= pack.city %></td>
        <td><%= pack.state %></td>
        <td><%= pack.population %></td>
        <td><%= link_to 'Show', pack %></td>
        <td><%= link_to 'Edit', edit_pack_path(pack) %></td>
        <td><%= link_to 'Destroy', pack, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

</table>
<%= will_paginate @packs %>
<br>

<%= link_to 'New Pack', new_pack_path %>
